{
  "name": "gfm-backend",
  "version": "1.0.0",
  "description": "GFM Terminal Backend API",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "build": "tsc",
    "postinstall": "npm rebuild && tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "backfill": "ts-node src/scripts/backfill.ts",
    "init-db": "ts-node src/scripts/init-db.ts",
    "sync": "ts-node src/scripts/sync.ts",
    "cron": "ts-node src/scripts/cron.ts",
    "test": "jest --config jest.config.ts --testPathPattern=src/__tests__",
    "test:watch": "jest --watch --config jest.config.ts --testPathPattern=src/__tests__",
    "test:coverage": "jest --coverage --config jest.config.ts --testPathPattern=src/__tests__"
  },
  "dependencies": {
    "@mendable/firecrawl-js": "1.16.0",
    "@supabase/supabase-js": "2.39.3",
    "@types/node-fetch": "2.6.11",
    "@vercel/node": "3.0.17",
    "axios": "1.6.5",
    "cors": "2.8.5",
    "dotenv": "16.4.7",
    "express": "4.18.2",
    "node-cron": "3.0.3",
    "resend": "4.1.2",
    "typescript": "5.3.3",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-typescript": "^7.22.0",
    "@babel/plugin-transform-modules-commonjs": "^7.22.0",
    "babel-jest": "^29.7.0",
    "@jest/globals": "29.7.0",
    "@jest/types": "29.6.3",
    "@types/body-parser": "1.19.5",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.11",
    "@types/node": "20.11.10",
    "@types/node-cron": "3.0.11",
    "jest": "29.7.0",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0"
  }
}
