{
  "crons": [{
    "path": "/api/sync",
    "schedule": "0 * * * *"
  }],
  "env": {
    "FIRECRAWL_API_KEY": "${FIRECRAWL_API_KEY}",
    "SUPABASE_URL": "${SUPABASE_URL}",
    "SUPABASE_ANON_KEY": "${SUPABASE_ANON_KEY}",
    "RESEND_API_KEY": "${RESEND_API_KEY}"
  },
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": "dist",
  "framework": "node"
} 